{
    "name": "sushilk/supabase",
    "description": "Supabase Client",
    "type": "library",
    "require": {
        "php": "^8.3",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "phpstan/phpstan": "^2.1",
        "laravel/pint": "^1.25"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Supabase\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Sushil Kumar",
            "email": "sushilkumbhar77@gmail.com"
        }
    ],
    "scripts": {
        "test:unit":"./vendor/bin/phpunit",
        "test:type": "./vendor/bin/phpstan analyse src --level=max",
        "test:lint": "./vendor/bin/pint",
        "tests": [
            "@test:lint",
            "@test:type",
            "@test:unit"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
